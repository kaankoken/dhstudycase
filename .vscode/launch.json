    "version": "0.2.0",
    "configurations": [
      {
        "name": "Run as Dev Debug",
        "request": "launch",
        "type": "dart",
        "program": "${workspaceFolder}/lib/main.dart",
        "args": [
          "--flavor",
          "dev"
        ],
        "preLaunchTask": "initDevConfig",
        "flutterMode": "debug"
      },
      {
        "name": "Run as Prod Debug",
        "request": "launch",
        "type": "dart",
        "program": "${workspaceFolder}/lib/main.dart",
        "args": [
          "--flavor",
          "prod"
        ],
        "preLaunchTask": "initProdConfig",
        "flutterMode": "debug"
      },
    ]
}